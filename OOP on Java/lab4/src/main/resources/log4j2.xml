<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <File name="File" fileName="logs/DealerInfo.log" append="false">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %-5p %c{1}:%L - %msg%n" />
        </File>
    </Appenders>

    <Loggers>
        <Root level="all">
            <AppenderRef ref="File" />
        </Root>
    </Loggers>
</Configuration>
